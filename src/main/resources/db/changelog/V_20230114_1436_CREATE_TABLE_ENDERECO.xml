<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
        
    <changeSet id="20230114-1438" author="rodrigo.bueno">
       <preConditions  onFail="MARK_RAN">
            <not>
              <tableExists tableName="endereco"/>
            </not>
        </preConditions>
       <createTable tableName="endereco">
          <column name="id" type="numeric(10,0)">
              <constraints nullable="false" primaryKey="true" primaryKeyName="pk_endereco"/>
          </column>
          <column name="cidade" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
          <column name="bairro" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
          <column name="rua" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
          <column name="numero" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
          <column name="complemento" type="varchar(500)"></column>
          <column name="observacoes" type="varchar(500)"></column>
       </createTable>
       <addAutoIncrement
          columnDataType="numeric(10,0)"
          columnName="id"
          tableName="endereco"/>
    </changeSet>
	
</databaseChangeLog>