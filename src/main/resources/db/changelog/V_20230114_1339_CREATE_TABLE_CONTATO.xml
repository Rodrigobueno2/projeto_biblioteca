<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
        
    <changeSet id="20230114-1340" author="rodrigo.bueno">
       <preConditions  onFail="MARK_RAN">
            <not>
              <tableExists tableName="contato"/>
            </not>
        </preConditions>
       <createTable tableName="contato">
          <column name="id" type="numeric(10,0)">
              <constraints nullable="false" primaryKey="true" primaryKeyName="pk_contato"/>
          </column>
          <column name="nome" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
          <column name="telefone" type="varchar(16)">
              <constraints nullable="false"/>
          </column>
          <column name="email" type="varchar(100)">
              <constraints nullable="false"/>
          </column>
       </createTable>
       <addAutoIncrement
          columnDataType="numeric(10,0)"
          columnName="id"
          tableName="contato"/>
    </changeSet>
	
</databaseChangeLog>